ifeq ($(PJ_PLATFORM), mediatek) 
CPPFLAGS += -DCONFIG_RALINK_MT7620
endif
ifeq ($(PJ_PLATFORM), mt7621) 
CPPFLAGS += -DCONFIG_RALINK_MT7621
endif
ifeq ($(PJ_PLATFORM), mt7688) 
CPPFLAGS += -DCONFIG_RALINK_MT7628#MT7688 and MT7628 use the same SDK, only differ in several configs
endif
ifeq ($(PJ_HWBOARD), cs5069)
CPPFLAGS += -DCONFIG_RALINK_CS5069
endif
ifeq ($(PJ_HWBOARD), cs5062)
CPPFLAGS += -DCONFIG_RALINK_CS5062
endif
ifeq ($(PJ_HWBOARD), dcs2630)
CPPFLAGS += -DCONFIG_RALINK_DCS2630
endif
ifeq ($(PJ_HWBOARD), cs6022)
CPPFLAGS += -DCONFIG_RALINK_CS6022
endif

